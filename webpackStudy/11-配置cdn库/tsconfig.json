{
  "compilerOptions": {
    "target": "es5",
    "jsxFactory": "h",
    "module": "es2015",
    "jsx": "preserve",
    "sourceMap": true,
    "importHelpers": true,
    "strict": false,
    "moduleResolution": "node",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}